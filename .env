# Twilio
TWILIO_ACCOUNT_SID=mock_sid_for_testing
TWILIO_AUTH_TOKEN=mock_token_for_testing
TWILIO_PHONE_NUMBER=+15551234567

# Groq API (Free LLM - sign up at https://console.groq.com)
GROK_API_KEY=
GROK_MODEL=mixtral-8x7b-32768
LLM_ENDPOINT=groq
LLM_MODEL=mixtral-8x7b-32768
LLM_API_KEY=

# Helius (Solana webhooks)
HELIUS_API_KEY=
HELIUS_WEBHOOK_URL=http://localhost:3000/helius-webhook

# Server
PORT=3000
NODE_ENV=development
LOG_LEVEL=debug

# Voice (Pipecat)
PIPECAT_DEBUG=false
VOICE_MODE=non-interactive

# Testing
DRY_RUN=false
MOCK_TWILIO=true

# Grok polling interval (ms)
GROK_POLL_INTERVAL=45000
GROK_RATE_LIMIT=1000

# Call slots (Phase 2) - JSON string
CALL_SLOTS=[{"start":"08:00","duration":3600},{"start":"14:00","duration":3600},{"start":"20:00","duration":3600}]

# Token gating (Phase 3)
TOKEN_PRICE_USD=2
SOLANA_BURN_ADDRESS=11111111111111111111111111111112

# Voice TTS/STT
TTS_ENGINE=chatterbox
STT_ENGINE=faster-whisper
STT_MODEL=large-v3

# Local or Remote GPU
VOICE_GPU_ENDPOINT=http://localhost:8000
VOICE_GPU_REMOTE=false

# Phase 2: Avatar
AVATAR_SERVICE=heygen
AVATAR_API_KEY=
AVATAR_ID=default_avatar
VOICE_ID=default_voice

# Phase 3: Solana Governance
SOLANA_RPC=https://api.mainnet-beta.solana.com
SOLANA_PROGRAM_ID=
TOKEN_MINT_ADDRESS=

# Phase 4: Akash Deployment
AKASH_PROVIDER_URL=http://localhost:3030
AKASH_ACCOUNT_ADDRESS=
AKASH_KEY_NAME=deployer

# Phase 4: Theta Streaming
THETA_API_URL=https://api.thetatoken.org/v2
THETA_API_KEY=
THETA_WALLET_ADDRESS=

# Database (PostgreSQL)
DATABASE_URL=postgresql://user:password@localhost:5432/cryptocall_fm
DATABASE_ENABLED=false

# Wallet Oracle
SOLANA_WALLET_FOR_ORACLE=
RAYDIUM_API_URL=https://api.raydium.io/v2
LOANSHARK_API_URL=https://api.loanshark.pro
